{
  "name": "@prisma-cms/front",
  "version": "1.4.0-beta",
  "description": "",
  "main": "lib/App.js",
  "module": "es/App.js",
  "files": [
    "src",
    "ssr",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-web-module --copy-files",
    "clean": "nwb clean-module",
    "prepublishOnly": "npm run build",
    "test": "nwb test",
    "test:coverage": "nwb test --coverage",
    "test:watch": "nwb test --server",
    "test-react-scripts": "node_modules/react-scripts/bin/react-scripts.js test",
    "start": "npm run start-js",
    "start-js": "react-scripts start",
    "start-ssr": "NODE_ENV=production node ssr",
    "start-ssr-dev": "NODE_ENV=production nodemon ssr",
    "build-front": "run-s -n build-css build-js",
    "build-js": "react-scripts build",
    "build-css": "run-p -ncr build-less",
    "build-less": "node-less-chokidar src/styles/less",
    "build-sass": "node-less-chokidar src/styles/sass -o src/styles/sass/",
    "watch-css": "run-p -ncr watch-less",
    "watch-less": "node-less-chokidar src/styles/less --watch",
    "watch-sass": "node-sass-chokidar src/styles/sass/ -o src/styles/sass/ --watch",
    "test-front": "run-s -n build-css test-js",
    "test-js": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prettier": "prettier-eslint --write \"src/**/*.+(js|jsx)\"",
    "prettier-dry-run": "prettier-eslint \"src/**/*.+(js|jsx)\" -l debug"
  },
  "dependencies": {
    "@material-ui/icons": "^3.0.1",
    "@prisma-cms/component": "^1.0.2",
    "@prisma-cms/connector": "^1.0.0",
    "@types/react": "^16.1.0",
    "apollo-boost": "^0.1.16",
    "apollo-cache-inmemory": "^1.2.10",
    "apollo-client": "^2.4.2",
    "apollo-cms": "^1.2.10",
    "apollo-error": "^1.0.2",
    "apollo-fetch-upload": "^1.2.0",
    "apollo-link": "^1.2.3",
    "apollo-link-batch-http": "^1.0.4",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "1.3.2",
    "apollo-link-ws": "^1.0.9",
    "apollo-upload-client": "^9.0.0",
    "apollo-utilities": "^1.0.21",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "debug": "^3.1.0",
    "dom-scroll-into-view": "^1.2.1",
    "draft-js": "^0.10.5",
    "graphql": "^0.13.2",
    "graphql-cli": "^2.15.13",
    "graphql-tag": "2.6.1",
    "jss-theme-reactor": "^0.11.1",
    "load-script": "^1.0.0",
    "material-ui": "1.0.0-beta.43",
    "material-ui-icons": "^1.0.0-beta.17",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "node-less-chokidar": "^0.1.2",
    "node-sass-chokidar": "^1.1.0",
    "prisma-binding": "2.1.5",
    "react": "^16.3.1",
    "react-apollo": "^2.2.3",
    "react-dom": "^16.3.0",
    "react-router-dom": "4.2.2",
    "shortid": "^2.2.8",
    "socket.io": "^2.0.4",
    "socket.io-parser": "^3.1.2",
    "subscriptions-transport-ws": "^0.9.5",
    "urijs": "^1.19.1",
    "xml-writer": "^1.7.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.26.0",
    "babel-runtime": "6.26.0",
    "enzyme": "^3.6.0",
    "nwb": "0.23.x",
    "prettier-eslint-cli": "4.7.0",
    "react-scripts": "2.0.1",
    "roddeh-i18n": "^1.1.1"
  },
  "author": "Fi1osof",
  "license": "MIT",
  "repository": {
    "type": "git",
