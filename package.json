{
  "name": "@prisma-cms/front",
  "version": "1.12.0",
  "private": false,
  "description": "",
  "main": "lib/App.js",
  "module": "es/App.js",
  "files": [
    "src",
    "ssr",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-web-module --copy-files",
    "build-app": "react-scripts build",
    "get-api-schema": "graphql get-schema -o src/schema/generated/api.graphql",
    "build-api-fragments": "node --experimental-modules src/server/scripts/deploy/build-api-fragments",
    "clean": "nwb clean-module",
    "prepublishOnly": "npm run build",
    "start": "npm run start-js",
    "start-js": "react-scripts start",
    "start-ssr": "NODE_ENV=production node ssr",
    "start-ssr-dev": "NODE_ENV=production nodemon ssr",
    "build-front": "run-s -n build-css build-js",
    "build-js": "react-scripts build",
    "build-css": "run-p -ncr build-less",
    "build-less": "node-less-chokidar src/styles/less",
    "build-sass": "node-less-chokidar src/styles/sass -o src/styles/sass/",
    "watch-css": "run-p -ncr watch-less",
    "watch-less": "node-less-chokidar src/styles/less --watch",
    "watch-sass": "node-sass-chokidar src/styles/sass/ -o src/styles/sass/ --watch",
    "test": "react-scripts test --env=jsdom",
    "test__": "nwb test",
    "test:coverage": "nwb test --coverage",
    "test:watch": "nwb test --server",
    "test-react-scripts": "node_modules/react-scripts/bin/react-scripts.js test",
    "test-front": "run-s -n build-css test-js",
    "test-js": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prettier": "prettier-eslint --write \"src/**/*.+(js|jsx)\"",
    "prettier-dry-run": "prettier-eslint \"src/**/*.+(js|jsx)\" -l debug"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@prisma-cms/component": "latest",
    "@prisma-cms/connector": "latest",
    "@prisma-cms/context": "latest",
    "@prisma-cms/editor": "latest",
    "@prisma-cms/graphql-voyager": "^1.0.0-rc.26",
    "@prisma-cms/ui": "latest",
    "@types/react": "^16.4.18",
    "apollo-boost": "^0.1.19",
    "apollo-cache-inmemory": "^1.3.8",
    "apollo-client": "^2.4.5",
    "apollo-cms": "latest",
    "apollo-error": "^1.0.2",
    "apollo-fetch-upload": "^1.2.0",
    "apollo-link": "^1.2.3",
    "apollo-link-batch-http": "^1.0.4",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "1.5.5",
    "apollo-link-ws": "^1.0.9",
    "apollo-upload-client": "^9.1.0",
    "apollo-utilities": "^1.0.25",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "debug": "^4.1.0",
    "dom-scroll-into-view": "^1.2.1",
    "draft-js": "^0.10.5",
    "graphql": "14.0.2",
    "graphql-cli": "^2.17.0",
    "graphql-tag": "2.10.0",
    "jss-theme-reactor": "^0.11.1",
    "load-script": "^1.0.0",
    "material-ui": "1.0.0-beta.43",
    "material-ui-icons": "^1.0.0-beta.17",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.23",
    "node-less-chokidar": "^0.3.1",
    "node-sass-chokidar": "^1.1.0",
    "prisma-binding": "^2.1.6",
    "react": "^16.7.0",
    "react-apollo": "^2.2.3",
    "react-dom": "^16.7.0",
    "react-router-dom": "4.3.1",
    "shortid": "^2.2.8",
    "socket.io": "^2.0.4",
    "socket.io-parser": "^3.1.2",
    "subscriptions-transport-ws": "^0.9.5",
    "urijs": "^1.19.1",
    "xml-writer": "^1.7.0"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@prisma-cms/server": "latest",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "babel-plugin-named-asset-import": "^0.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^6.1.0",
    "babel-preset-react-hmre": "^1.1.1",
    "enzyme": "^3.6.0",
    "nwb": "0.23.x",
    "prettier-eslint-cli": "4.7.1",
    "react-scripts": "^2.1.2",
    "roddeh-i18n": "^1.1.1"
  },
  "resolutions": {
    "graphql": "14.0.2"
  },
  "author": "Fi1osof",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prisma-cms/front.git"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "keywords": [
    "prisma",
    "cms",
    "frontend"
  ],
  "bugs": {
    "url": "https://github.com/prisma-cms/front/issues"
  },
  "homepage": "https://github.com/prisma-cms/front#readme"
}
